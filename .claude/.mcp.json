{
  "mcpServers": {
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest",
        "--browser",
        "firefox"
      ]
    },
    "serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant"
      ]
    },
    "docker-mcp": {
      "command": "uvx",
      "args": [
        "docker-mcp"
      ]
    },
    "filesystem": {
      "command": "/home/kblack0610/dev/home/binks-agent-orchestrator/target/release/filesystem-mcp",
      "args": [],
      "env": {
        "RUST_LOG": "info"
      }
    },
    "memory": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "-v",
        "claude-memory:/app/dist",
        "--rm",
        "mcp/memory"
      ]
    },
    "github-gh": {
      "command": "/home/kblack0610/dev/home/binks-agent-orchestrator/mcps/github-gh/target/release/github-gh-mcp",
      "args": [],
      "env": {
        "RUST_LOG": "info"
      }
    },
    "web-search": {
      "command": "/home/kblack0610/dev/home/binks-agent-orchestrator/target/release/web-search-mcp",
      "args": [],
      "env": {
        "SEARXNG_URL": "http://localhost:8888",
        "RUST_LOG": "info"
      }
    },
    "agent": {
      "command": "/home/kblack0610/dev/home/binks-agent-orchestrator/target/release/agent",
      "args": [
        "serve"
      ],
      "env": {
        "RUST_LOG": "info"
      }
    }
  }
}
