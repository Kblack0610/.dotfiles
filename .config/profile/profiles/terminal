# Terminal Profile
# TTY-only mode, no graphical environment

PROFILE_NAME="terminal"
PROFILE_DESCRIPTION="Terminal-only mode, no GUI"

# Login behavior
PROFILE_AUTOLOGIN=true

# Session startup
PROFILE_START_HYPRLAND=false

# Services to enable
PROFILE_START_SUNSHINE=false
PROFILE_START_WAYBAR=false

# Additional options
PROFILE_ENABLE_SSH=true
