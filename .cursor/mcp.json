{
  "mcpServers": {
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "env": {}
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@executeautomation/playwright-mcp-server"
      ],
      "env": {}
    },
    "python-execution": {
      "command": "uv",
      "args": [
        "run",
        "python",
        "-m",
        "mcp_python"
      ],
      "env": {}
    },
    "filesystem": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--mount",
        "type=bind,src=/home/kblack0610,dst=/projects/kblack0610",
        "mcp/filesystem",
        "/projects"
      ]
    }
  }
}
